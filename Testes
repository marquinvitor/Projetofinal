import java.util.*;

enum Turno {
    MANHA,
    TARDE,
    NOITE;

    @Override
    public String toString() {
        return this.name().toLowerCase();
    }
}

class Students {
    int idade;
    String nome;
    Turno turno;

    public Students(int age, String name, Turno turno) {
        this.setIdade(age);
        this.setNome(name);
        this.turno = turno;

        if (this.idade > 50) {
            throw new RuntimeException("fail: idade incompativel");
        }
    }

    public int getIdade() {
        return this.idade;
    }

    public String getnome() {
        return this.nome;
    }

    public void setIdade(int idade) {
        this.idade = idade;
    }

    public void setNome(String nome) {
        this.nome = nome;
    }

    public String toString() {
        return "nome: " + this.nome + " | idade: " + this.idade + " | turno: " + this.turno;
    }
}

class Lista {
    ArrayList<Students> estudantess;

    public Lista(int qtd) {
        this.estudantess = new ArrayList<>(qtd);

        for (int i = 0; i < qtd; i++) {
            this.estudantess.add(new Students(0, "empty", null));
        }
    }

    public void ordenarPorNome() {
        Collections.sort(estudantess, new Comparator<Students>() {
            public int compare(Students s1, Students s2) {
                return s1.nome.compareTo(s2.nome);
            }
        });
    }

    public void setAluno(int ind, int idade, String nome, Turno turno) {
        if (ind >= 0 && ind < this.estudantess.size()) {
            Students estudante = new Students(idade, nome, turno);
            this.estudantess.set(ind, estudante);
        } else {
            throw new RuntimeException("fail: indice incorreto");
        }
    }

    public String toString() {
        String saida = "lista [ ";
        for (Students student : this.estudantess) {
            if (student != null) {
                saida += "(" + student.toString() + ") ";
            }
        }
        saida += "]";
        return saida;
    }

}

public class Testes {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        Lista lista = new Lista(0);

        while (true) {
            String linha = scanner.nextLine();
            String ui[] = linha.split(" ");
            System.out.println("$" + linha);

            try {

                if (ui[0].equals("end")) {
                    break;
                } else if (ui[0].equals("init")) {
                    lista = new Lista(Integer.parseInt(ui[1]));
                } else if (ui[0].equals("show")) {
                    System.out.println(lista);
                } else if (ui[0].equals("adicionar")) {
                    lista.setAluno(Integer.parseInt(ui[1]), Integer.parseInt(ui[2]), ui[3],
                            Turno.valueOf(ui[4].toUpperCase()));
                } else if (ui[0].equals("ordenar")) {
                    lista.ordenarPorNome();
                }

            } catch (RuntimeException e) {
                System.out.println(e.getMessage());
            }

        }
    }
}
